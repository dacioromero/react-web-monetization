{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "jsx": "react",
    "types": [
      "react",
      "react-dom",
      "node"
    ],
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declarationMap": true,
    "sourceMap": true,
    "noImplicitAny": true
  }
}
